{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs", /* Specify what module code is generated. */
    "rootDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": [
    "scripts/**/*", "codegen.ts"
  ]
}